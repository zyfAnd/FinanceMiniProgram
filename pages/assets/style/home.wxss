.home_userinfo {
  text-align: center;
  margin: 0 auto;
  width: 350rpx;
  height: 350rpx;
  margin-left: 100rpx;
  margin-right: 100rpx;
   margin-top: 100rpx; 

}

.home_userinfo>navigator, .home_userinfo>view:last-child {
  height: 50rpx;
  color: #fff;
}

.home_grade {
  line-height: 45rpx;
  background-size: 49rpx 50rpx;
  padding-left: 50rpx;
  width: 120rpx;
  font-size: small;
  background-repeat: no-repeat;
  background-position: center left;
  background-image: url("http://pcp-guanjiastatics.oss-cn-shanghai.aliyuncs.com/xcx_pass/icon_home_grade.png");
}
.home_grade .grade {
  display: inline-block;
}

.home_grade text {
  padding: 0 20rpx 0 0;
}

.home_grade:after {
  content: '';
  font-size: 0;
  width: 0;
  height: 0;
  border-top: 12rpx solid transparent;
  border-bottom: 12rpx solid transparent;
  border-left: 12rpx solid #dfe8ff;
  vertical-align: middle;
}

.home_score {
  line-height: 50rpx;
  background-size: 50rpx 47rpx;
  padding-left: 50rpx;
  width: 120rpx;
  font-size: small;
  background-repeat: no-repeat;
  background-position: center left;
  background-image: url("http://pcp-guanjiastatics.oss-cn-shanghai.aliyuncs.com/xcx_pass/icon_home_score.png");
}

.home_share {
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  width: 60rpx;
  height: 60rpx;
  background: url("http://pcp-guanjiastatics.oss-cn-shanghai.aliyuncs.com/xcx_pass/icon_home_share.png") no-repeat;
  background-size: cover;
}

.home_gate {
  height: 280rpx;
  /* background: linear-gradient(to bottom right, #ff6f6f, #ff5757); */
    border-radius: 15rpx;  
    display: flex;  
  text-align: center;
  align-items: center;
  /* justify-content: space-between; */
    /* padding: 0 40rpx;   */
  margin: 30rpx 0;
}

.home_gate_text text, .home_rank text, .home_power text {
  color: #ffefef;
  display: flex;
  text-shadow: #fb5454;
}

.home_gate_text text:first-child {
  font-size: 48rpx;
}

.home_gate_text text:last-child {
  font-size: small;
  margin-top: 20rpx;
}

.home_gate_img image {
  width: 200rpx;
  height: 164rpx;
}

.rankAndPower {
  display: flex;
  height: 250rpx;
  justify-content: space-between;
}

.home_rank {
  width: 260rpx;
  background: linear-gradient(to bottom right, #976aeb, #8654e5);
  border-radius: 15rpx;
  padding: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

.home_rank text {
  align-self: flex-start;
}

.home_rank image {
  width: 237rpx;
  height: 133rpx;
}

.home_power {
  width: 280rpx;
  background: linear-gradient(to bottom right, #47c920, #3fb71b);
  border-radius: 15rpx;
  padding: 20rpx 0 0 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.home_power image {
  width: 171rpx;
  height: 184rpx;
  align-self: flex-end;
}

.topcrown {
  position: absolute;
  top: -30rpx;
  width: 83rpx;
  height: 61rpx;
  left: 0;
  right: 0;
  margin: auto;
  border: 0;
  border-radius: 0;
  z-index:999;
}

.home_userinfo .charts_avatar {
  position: relative;
}
.charts_avatar navigator,.power{
  display:flex;
  flex-direction:column;
  align-items:center;
  width:200rpx;
}
.charts_avatar navigator text,.power text{
  overflow:hidden;
  white-space:nowrap;
  text-overflow: ellipsis;
  max-width:200rpx;
}
